<script type="text/javascript">
    jQuery(function() {
        if (localStorage.getItem('cookies') == null) {
            $('.cookies').show();
        }        
        
        $('.cookies button').on('click', function() {
            $('.cookies').hide();
            localStorage.setItem('cookies', 'accepted');
        });        
        
        $('.btnAcceptCookies').on('click', function() {
            $('.cookies').hide();
            localStorage.setItem('cookies', 'accepted');
        });
    });

